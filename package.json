{
  "name": "Simple-Static-React-Firebase",
  "version": "1.0.2",
  "description": "Minimal boilerplate and configuration for the development of react sites backed by firebase",
  "repository": "https://github.com/Jon-Biz/simple-static-react-firebase",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "npm run webpack",
    "dev": "npm run webpack-dev-server -s -- --progress --port 4000"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "jon-biz",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.1",
    "debug": ">=2.6.9",
    "js-yaml": "^3.13.1",
    "mini-css-extract-plugin": "^2.3.0",
    "mixin-deep": ">=1.3.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "reduce-css-calc": ">=1.2.5",
    "set-value": ">=4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
